<jqa:jqassistant-rules xmlns:jqa="http://www.buschmais.com/jqassistant/core/analysis/rules/schema/v1.0">

    <concept id="test:DeclaredMembers.graphml">
        <description>A type and its declarations</description>
        <cypher><![CDATA[
            MATCH
              (t:Class)-[r:DECLARES]->(m)
            RETURN
              t, r as rel, m
        ]]></cypher>
    </concept>

    <concept id="test:DeclaredMembersWithVirtualRelation.graphml">
        <description>A type and its declarations</description>
        <cypher><![CDATA[
            MATCH
              (t:Class)-[r:DECLARES]->(m)
            RETURN
              t, {type:"DECLARES", startNode:startNode(r), endNode:endNode(r), properties: { "name" : m.name }} as rel, m
        ]]></cypher>
    </concept>
</jqa:jqassistant-rules>
